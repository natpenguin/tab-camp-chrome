{
  "manifest_version": 2,
  "name": "Tab Camp",
  "version": "1.0.0",
  "description": "Very easy to quickly switch list of tabs.",
  "icons": {
    "16": "resources/icons/icon16.png",
    "48": "resources/icons/icon48.png",
    "128": "resources/icons/icon128.png"
  },
  "permissions": ["tabs", "storage"],
  "browser_action": {
    "default_popup": "src/popup/index.html"
  }
}